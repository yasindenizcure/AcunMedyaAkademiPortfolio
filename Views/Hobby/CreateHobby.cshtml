@model AcunMedyaAkademiPortfolio.Models.TblHobby
@{
    ViewBag.Title = "CreateHobby";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<h2>Yeni Hobi Ekleme</h2>
<form method="post">
    <label>Hobi ID</label>
    @Html.TextBoxFor(x => x.HobbyId, new { @class = "form-control" })

    <label>Icon Url</label>
    @Html.TextBoxFor(x => x.IconUrl, new { @class = "form-control" })

    <label>Başlık</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })

    <br />
    <button class="btn btn-info">Hobiyi Kaydet</button>
</form>
